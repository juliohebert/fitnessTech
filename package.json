  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\" -n \"CLIENT,SERVER\" -c \"cyan,green\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/server.ts",
    "build": "prisma generate && vite build",
    "build:server": "tsc src/server.ts --outDir dist",
    "start": "node dist/server.js",
    "preview": "vite preview",
    "vercel-build": "prisma generate && prisma db push --force-reset --skip-generate && vite build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@google/genai": "^1.39.0",
    "@google/generative-ai": "^0.24.1",
